public class primenumber
{
public static void main(string[] rags){
 int num=3;
 int count=0;
 if(num>1)
 {
  for(int i=1;i<=num;i++)
  {
    if(num%1==0)
       count++;
   }
   if(count==2)
   {
     system.out.println("prime number");
    }
    else
    {
     system.out.println("Not prime number");
     }
   }
   else
   {
     system.out.println("Not prime number");
    }
  }
}
